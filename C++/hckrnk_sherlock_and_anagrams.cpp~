#include <iostream>
#include <string>
#include <vector>
using namespace std;
void sherlock_anagrams(string str){
  int n=str.size();
  vector<string> anagrams;
  for(int i=1;i<n;i++){
    int sub_len=n-i;
    for(j=0;j+i<n-sub_len+1;j++){
      string sub=str.substr(j,sub_len);
      anagrams.push_back(sub);
      cout<<endl<<sub;
    }
  }
}
int main(int argc, char *argv[]){
  int q;
  cin>>q;
  for (i=0; i<q; i++) {
    string str;
    cin>>str;
    sherlock_anagrams(str);
  }

  return 0;
}
