//TODO
#include <iostream>
using namespace std;
int main()
{
  cin.tie(0);
  ios::sync_with_stdio(0);
  int inv_count;
  int t,n;
  cin>>t;
  for(int i=0;i<t;i++){
    cin>>n;
    int arr[n];
    for(int j=0;j<n;j++)
      cin>>arr[j];
    for(int i=0;i<n-1;i++){
      if(arr[i]<arr[i+1])
	inv_count++;
    }
    if(inv_count%2==0)
      cout<<"YES\n";
    else
      cout<<"NO\n";
  }
  return 0;
}
