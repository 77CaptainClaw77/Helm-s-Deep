#include <stdio.h>
#include <stdlib.h>
int* comparator(void *arg1,void *arg2)
{
  return *((int*)arg1)>*((int*)arg2);
}
int main()
{
  int n,i;
  printf("Enter the number of elements:");
  scanf("%d",&n);
  int *arr=malloc(sizeof(int)*n);
  for(i=0;i<n;i++)
    scanf("%d",&arr[i]);
  qsort(arr,n,sizeof(int),comparator);
  return 0;
}
