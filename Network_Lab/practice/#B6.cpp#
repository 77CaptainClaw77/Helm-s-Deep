#include <bits/stdc++.h>
string get_codeword(string dtw,string gen){
  for(int i=0;i<gen.size()-1;i++)
    dtw=dtw+"0";
  
}
string divide(string dividend,string divisor){
  int dd=dividend.size();
  int dv=divisor.size();
  for(int i=0;i<dd-dv+1;i++){
    for(int j=0;j<dv;j++)
      dv[j]=dv[j]==dd[i+j]?'0':'1';
  }
}
int main(int argc, char *argv[])
{
  string gen,dtw;
  cout<<"Enter dataword: ";
  cin>>dtw;
  cout<<"Enter generator: ";
  cin>>gen;
  
  return 0;
}
